[alias]
  aa = add .
  ba = branch -a
  c = checkout
  cb = checkout -b
  s = switch
  sm = switch -
  sc = switch -c
  update = "!f() { git fetch $1; git rebase $1/$2; }; f"
  cm = commit -m
  cam = commit --amend -a -C HEAD
  fixit = commit --amend --no-edit
  st = status
  f = fetch
  r = rebase
  lo = log -n 1
  po = push origin HEAD
  pf = push --force-with-lease origin HEAD

